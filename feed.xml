---
layout: null
---


<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
	<channel>
        <title>{{ site.title }}</title>
        <link>{{ site.url }}</link>
		<description>文韬的博客</description>
		{% for post in site.posts %}
		<item>
			<description>
				{{ post.content }}
			</description>
			<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
			<guid>
				{{ post.url | prepend: site.url }}
			</guid>
		</item>
		{% endfor %}
	</channel>
</rss>
